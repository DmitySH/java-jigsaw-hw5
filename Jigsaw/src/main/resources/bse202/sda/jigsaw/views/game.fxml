<?xml version="1.0" encoding="UTF-8"?>


<?import java.net.URL?>
<?import bse202.sda.jigsaw.models.fxml.Field?>
<?import javafx.scene.layout.Pane?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>

<VBox fx:id="mainBox"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="bse202.sda.jigsaw.controllers.GameController"
      alignment="TOP_CENTER"
      style="-fx-padding: 60 60 60 60"
      spacing="100">
    <stylesheets>
        <URL value="@css/styles.css"/>
    </stylesheets>
    <Pane fx:id="pane" prefWidth="540" maxWidth="540" minWidth="540" maxHeight="540">
        <Field fx:id="field" id="field" rows="9" columns="9" cellSize="60"/>
    </Pane>
    <HBox fx:id="figuresSpawn"
          style="-fx-spacing: 70; -fx-background-color: red"
          alignment="CENTER">
    </HBox>
</VBox>



