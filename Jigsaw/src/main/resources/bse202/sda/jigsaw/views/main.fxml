<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox alignment="TOP_CENTER"
      prefHeight="610"
      prefWidth="370"
      spacing="30"
      xmlns="http://javafx.com/javafx/17"
      fx:controller="bse202.sda.jigsaw.controllers.MainController"
      xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets top="200"/>
    </padding>
    <Button fx:id="startButton" alignment="TOP_LEFT"
            mnemonicParsing="false" text="PLAY"
            onAction="#createGameWindow"/>
    <Button fx:id="quitButton" mnemonicParsing="false" text="QUIT"
            onAction="#closeApp"/>
</VBox>
